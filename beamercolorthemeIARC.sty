    \ProvidesPackage{beamercolorthemeIARC}[01/11/2010 v0.1 beamer 'IARC' colour theme]

    \mode<presentation>

    \definecolor{cardinalred}{RGB}{140,21,21}
    \definecolor{stanfordgreen}{RGB}{0,155,118}
    \definecolor{stanfordorange}{RGB}{233,131,0}
    \definecolor{darkgray}{RGB}{77,79,83}
    \definecolor{lightgray}{RGB}{182,177,169}
    %neutral palette
    \definecolor{beige}{RGB}{210,194,149}
    \definecolor{darkbeige}{RGB}{179,153,93}

	\definecolor{IARCblue}{RGB}{72, 185, 238}
	\definecolor{IARCdblue}{RGB}{31,31,153}

	\setbeamercolor{reverse}{bg=white,fg=black}
    \setbeamercolor{normal text}{fg=black,bg=white}
    \setbeamercolor{alerted text}{fg=stanfordorange}
    \setbeamercolor{example text}{fg=IARCdblue}
    \setbeamercolor{structure}{fg=IARCblue}
    \setbeamercolor{section in head/foot}{bg=IARCdblue,fg=white}
    \setbeamercolor{section}{bg=IARCdblue,fg=white}
    \setbeamercolor{subsection in head/foot}{bg=white,fg=black}

   \setbeamercolor{title}{fg=IARCdblue,bg=IARCblue}
   \setbeamercolor{author}{fg=lightgray}
    \setbeamercolor{date}{fg=lightgray}
     \setbeamercolor{institute}{fg=IARCblue}
      \setbeamercolor{part}{fg=IARCblue}
    \setbeamercolor{framesubtitle}{fg=lightgray}

    \mode<all>

